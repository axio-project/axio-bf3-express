{
  "doca_general_flags":{

    // -l - sets the log level for the application DEBUG=60, CRITICAL=20
    "log-level": 60,
  },
  "doca_program_flags":{

    // -file - for client - file to send. for server - path to write the file to
    "file": "received_file.txt",

    // -p - device PCI address
    "pci-addr": "03:00.0",

    // -r - representor PCI address for the server
    "rep-pci": "3b:00.0",

    // -t - timeout when receiving the file data in the server (in seconds)
    "timeout": 2
  }
}
